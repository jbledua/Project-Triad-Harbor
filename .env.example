# Stack identity
STACK_NAME=triad-harbor
TIMEZONE=America/Toronto

# Tailscale
TS_HOSTNAME=triad-harbor
TS_AUTHKEY=tskey-auth-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Web UI exposure
# qBittorrent WebUI listens on 8080 inside the shared namespace.
# This publishes it on the Docker host at the port below.
PUBLIC_PORT_WEBUI=8080

# NordVPN credentials (OpenVPN)
# Use service credentials if NordVPN provides them for OpenVPN, otherwise your normal login.
NORDVPN_USER=your_nordvpn_username
NORDVPN_PASS=your_nordvpn_password

# Optional VPN server filters
 VPN_COUNTRY=Canada
 VPN_REGION=Ontario
 VPN_CITY=Toronto

# LinuxServer container permissions (match your NAS user)
PUID=1000
PGID=1000
